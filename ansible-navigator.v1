---
ansible-navigator:
  ansible:
    config: ./ansible.cfg
  execution-environment:
    container-engine: podman
    enabled: false
    image: ee-supported-rhel8
    pull:
      policy: never
  logging:
    level: debug
  playbook-artifact:
    save-as: /home/ansibleadmin/playbook-artifacts/{playbook_name}-artifact-{time_stamp}.json
